package testinium;

import java.util.ArrayList;
import java.util.List;

public class Task {

	public static void main(String[] args) {
		
		List<String> list= new ArrayList<String>();
		
		int counter=100;
		do {
			list.add("*");
			counter--;
		} while (counter>0);
		
		
		for(int i=0;i<100;i++) {
			for(int j=1;j<100;j++) {
				
				int size=list.size();
				System.out.print(list);
				list.set(j, "");
				list.set(size-j, "");
				System.out.println(size+"size");
				
			}	System.out.println();
		}
	}
	
}
