package testinium;

import java.util.ArrayList;
import java.util.List;

public class Task {

	public static void main(String[] args) {
		
		List<String> list= new ArrayList<String>();
		
		int counter=100;
		do {
			list.add("*");
			counter--;
		} while (counter>0);
		
		
			for(int j=1;j<99;j--) {
				
				int size=list.size();
				System.out.print(list.get(j));
				list.set(j, "");
				list.set(size-j, "");
				System.out.println();
			}	
		
	}
	
}
