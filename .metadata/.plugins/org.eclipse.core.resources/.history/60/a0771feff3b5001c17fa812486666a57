package testinium;

import java.util.ArrayList;
import java.util.List;

public class Task {

	public static void main(String[] args) {

		List<String> list = new ArrayList<String>();

		int counter = 100;
		do {
			list.add("*");
			counter--;
		} while (counter > 0);

		int size = list.size();
		for (int i = 0; i < 100; i++) {
			for (int j = 0; j < 99; j++) {

				System.out.print(list.get(j));

			}
			list.set(i, "");
			list.set(size - i, "");

			System.out.println();
		}
	}

}
