package testinium;

import java.util.ArrayList;
import java.util.List;

public class Task {

	public static void main(String[] args) {
		
		List<String> list= new ArrayList<String>();
		
		int counter=100;
		
		do {
			list.add("*");
			counter--;
		} while (counter>0);
		
		System.out.println(list.spliterator().toString());
	}
	
}
